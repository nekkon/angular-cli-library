{
  "name": "angular-cli-library",
  "version": "1.5.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "library:publish": "npm publish src/shared",
    "update": "node scripts/update.js",
    "verify": "node scripts/verify.js",
    "app1": "ng serve --app app1-browser --port 4200",
    "app2": "ng serve --app app2-browser --port 4201",
    "app1:build": "ng build --app app1-browser --prod --build-optimizer",
    "app2:build": "ng build --app app2-browser --prod --build-optimizer",
    "app1:ssr": "npm run app1:build && npm run app1:ssr:build && webpack --config webpack.app1.server.config.js --progress --colors && node dist/app1-server/server.js",
    "app2:ssr": "npm run app2:build && npm run app2:ssr:build && webpack --config webpack.app2.server.config.js --progress --colors && node dist/app2-server/server.js",
    "app1:ssr:build": "ng build --app app1-server --prod --output-hashing=false",
    "app2:ssr:build": "ng build --app app2-server --prod --output-hashing=false",
    "app1:test": "ng test --app app1",
    "app2:test": "ng test --app app2",
    "app1:e2e": "ng e2e --app app1",
    "app2:e2e": "ng e2e --app app2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.3",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@angular/platform-server": "5.0.3",
    "@angular/router": "5.0.3",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0-beta.5",
    "angular-cli-library": "1.3.2",
    "core-js": "2.5.1",
    "rxjs": "5.5.2",
    "ts-loader": "3.1.1",
    "@types/node": "8.0.53",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.4",
    "@angular/compiler-cli": "5.0.3",
    "@angular/language-service": "5.0.3",
    "@types/jasmine": "2.8.2",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.0.53",
    "codelyzer": "4.0.1",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.4.2"
  }
}
